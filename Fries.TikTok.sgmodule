#!arguments=CountryCode:TW,Carrier:"中華電信"
#!arguments-desc=CountryCode: 国家或地区代码\nCarrier: 网络运营商

[Script]
Fries.TikTok.request = type=http-request, pattern=^https?:\/\/(.+)\.tiktokv\.(com|us|eu)\/, requires-body=1, script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/main/js/Fries.TikTok.request.js

Fries.TikTok.tnc.get_domains.response = type=http-response, pattern=^https?:\/\/tnc(.+)\.tiktokv\.com\/get_domains\/v\d\/, requires-body=1, script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/main/js/Fries.TikTok.response.js
Fries.TikTok.api.aweme.v2.feed.response = type=http-response, pattern=^https?:\/\/api(.+)\.tiktokv\.com\/aweme\/v2\/feed\/, requires-body=1, binary-body-mode=1, script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/main/js/Fries.TikTok.response.js
Fries.TikTok.api.response = type=http-response, pattern=^https?:\/\/api(.+)\.tiktokv\.com\/(aweme|tiktok)\/v\d\/, requires-body=1, max-size=2097152, script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/main/js/Fries.TikTok.response.js
Fries.TikTok.response = type=http-response, pattern=^https?:\/\/(.+)\.tiktokv\.(com|us|eu)\/, requires-body=1, script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/main/js/Fries.TikTok.response.js

Fries.TikTok.request = type=http-request, pattern=^https?:\/\/(.+)\.byteoversea\.com\/, requires-body=1, script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/main/js/Fries.TikTok.request.js

[MITM]
hostname = %APPEND% *.tiktokv.com, *.tiktokv.us, *.tiktokv.eu, *.byteoversea.com
